{
  "name": "objectypes",
  "version": "1.2.3",
  "description": "A type-safe library to transform and validate objects",
  "files": [
    "dist"
  ],
  "main": "dist",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/marquesVF/objectypes"
  },
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "prepare": "npm run build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .ts --cache lib",
    "lint:prettier": "prettier \"**/*.{ts,md}\"",
    "fix": "npm run lint:prettier --write && npm run lint:eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "type-safety",
    "mapper",
    "json",
    "converter",
    "typescript"
  ],
  "author": "Vin√≠cius de F. Marques",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/mocha": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "25.5.4",
    "lodash": "4.17.21",
    "prettier": "^2.3.0",
    "ts-jest": "25.5.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/ramda": "0.27.7",
    "ramda": "0.27.0",
    "reflect-metadata": "0.1.13"
  }
}
